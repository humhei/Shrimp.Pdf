<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Configurations>Debug;Release;MiniBuild</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='MiniBuild|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Resources\Marks\*.pdf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="Resources\Colors\*.pdf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="Resources\Colors\*\*.pdf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>

    <None Update="Resources\Colors\registion.pdf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <Compile Include="Marks.fs" />
    <Compile Include="Pantone.fs" />
    <Compile Include="Colors.fs" />
    <Compile Include="Types.fs" />
    <Compile Include="Filters.fs" />
    <Compile Include="Select.fs" />
    <Compile Include="Operators.fs" />
    <Compile Include="Modify.fs" />
    <Compile Include="PageReader.fs" />
    <Compile Include="DocReader.fs" />
    <Compile Include="Manipulates.fs" />
    <Compile Include="Reuses.fs" />
    <Compile Include="Elmish.fs" />

  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BlackFox.ColoredPrintf" Version="1.0.2" />
    <PackageReference Include="Fake.IO.FileSystem" Version="5.0.0-rc012.95" />
    <PackageReference Include="FParsec" Version="1.0.3" />
    <PackageReference Include="LiteDB.FSharp" Version="2.6.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Shrimp.Pdf.Extensions\Shrimp.Pdf.Extensions.fsproj" />
    <ProjectReference Include="..\Shrimp.Pdf.Logger\Shrimp.Pdf.Logger.fsproj" />
    <ProjectReference Include="..\Shrimp.Pdf.Parser\Shrimp.Pdf.Parser.fsproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="4.5.2" />
    <PackageReference Update="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

</Project>